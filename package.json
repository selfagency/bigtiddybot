{
  "name": "bigtiddybot",
  "version": "0.0.1",
  "description": "a bot that toots big tiddies",
  "main": "dist/index.js",
  "repository": "https://bitbucket.org/selfagency/bigtiddybot/",
  "author": "Daniel Sieradski <daniel@self.agency>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "./node_modules/.bin/concurrently ./node_modules/.bin/ndb './node_modules/.bin/ndb nodemon --config .nodemonrc ./app.js' 'yarn webpack-watch'",
    "start": "start ./dist/index.js",
    "test": "jest tests/* --forceExit",
    "ctest": "jest tests/* --coverage --forceExit && cat ./coverage/lcov.info | ./node_modules/.bin/env-cmd ./.env codacy-coverage",
    "webpack": "webpack --config ./webpack.config.js --mode development",
    "webpack-watch": "webpack --config ./webpack.config.js --mode development --watch",
    "webpack-build": "webpack --config ./webpack.config.js --mode production"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "eslint": "^5.4.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.5.0",
    "ndb": "^1.0.24",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "emoji.json": "^11.0.0",
    "envkey": "^1.1.3",
    "mastodon-api": "^1.3.0"
  }
}
